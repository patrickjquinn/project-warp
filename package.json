{
	"version": "0.0.0",
	"type": "module",
	"main": "dist/elec/build.cjs",
	"scripts": {
		"dev": "vite",
		"dev:all": "concurrently -n=svelte,elec -c=green,blue \"pnpm dev\" \"pnpm dev:elec\"",
		"dev:elec": "node script/build --env=development --watch",
		"build": "vite build",
		"serve": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"lint:fix": "pnpm format && eslint --fix --ignore-path .gitignore .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@rollup/plugin-alias": "^3.1.2",
		"@rollup/plugin-commonjs": "^19.0.0",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^13.0.0",
		"@sveltejs/vite-plugin-svelte": "^1.0.0-next.12",
		"@tsconfig/svelte": "^2.0.1",
		"@types/node": "^16.3.1",
		"@typescript-eslint/eslint-plugin": "^4.28.2",
		"@typescript-eslint/parser": "^4.28.2",
		"chalk": "^4.1.1",
		"concurrently": "^6.2.0",
		"default-shell": "^1.0.1",
		"dotenv": "^10.0.0",
		"electron": "^13.1.6",
		"electron-builder": "^22.11.7",
		"electron-connect": "^0.6.3",
		"electron-rebuild": "^2.3.5",
		"eslint": "^7.30.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.0",
		"minimist": "^1.2.5",
		"monaco-editor": "^0.25.2",
		"ora": "^5.4.1",
		"prettier": "~2.3.2",
		"prettier-plugin-svelte": "^2.3.1",
		"pretty-ms": "^7.0.1",
		"rollup": "^2.53.1",
		"rollup-plugin-esbuild": "^4.5.0",
		"svelte": "^3.38.3",
		"svelte-awesome": "^2.3.2",
		"svelte-check": "^2.2.2",
		"svelte-dnd-action": "^0.9.9",
		"svelte-preprocess": "^4.7.4",
		"svelte-split-pane": "^0.1.2",
		"svelte-tabs": "^1.1.0",
		"svelte-tree": "^0.7.0",
		"svelte-watch-resize": "^1.0.3",
		"tiny-glob": "^0.2.9",
		"tslib": "^2.3.0",
		"typescript": "^4.3.5",
		"vite": "^2.4.1",
		"vite-plugin-commonjs-externals": "^0.1.1",
		"wait-on": "^6.0.0"
	},
	"dependencies": {
		"directory-tree": "^2.2.9",
		"electron-is-dev": "^2.0.0",
		"electron-store": "^8.0.0",
		"glob": "^7.1.7",
		"node-pty": "^0.10.1",
		"xterm": "^4.13.0",
		"xterm-addon-fit": "^0.5.0",
		"xterm-webfont": "^2.0.0"
	}
